#include <iostream>
#include <vector>

using namespace std;

int lowerBound(const vector<int>& nums, int x) {
    int low = 0;
    int high = nums.size();

    while (low < high) {
        int mid = low + (high - low) / 2;

        if (nums[mid] >= x) {
            high = mid;
        } else {
            low = mid + 1;
        }
    }
    return low;
}

int main() {
    int n;
    if (cin >> n) {
        vector<int> nums(n);
        for (int i = 0; i < n; i++) {
            cin >> nums[i];
        }

        int x;
        cin >> x;

        cout << lowerBound(nums, x) << endl;
    }
    return 0;
}
