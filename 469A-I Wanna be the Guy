#include <bits/stdc++.h>
using namespace std;
int main(){
    int n; cin >> n;
    vector<int> seen(n+1, 0);
    int p; cin >> p;
    while(p--){ int x; cin >> x; seen[x]=1; }
    int q; cin >> q;
    while(q--){ int x; cin >> x; seen[x]=1; }
    for(int i=1;i<=n;i++)
        if(!seen[i]){
            cout << "Oh, my keyboard!";
            return 0;
        }
    cout << "I become the guy.";
}
